{
  "name": "taiga-contrib-hipchat",
  "version": "0.0.1",
  "description": "Taiga contrib plugin for hipchat integration",
  "main": "index.js",
  "scripts": {
    "build-jade": "node_modules/jade/bin/jade.js partials/contrib/hipchat.jade -o dist/",
    "build-coffee": "cat coffee/hipchat.coffee dist/hipchat.html | node_modules/coffee-script/bin/coffee --compile --stdio > dist/hipchat.js",
    "build": "npm run build-jade; npm run build-coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taigaio/taiga-contrib-hipchat"
  },
  "keywords": [
    "hipchat",
    "taiga",
    "integration"
  ],
  "author": "Andrea Stagi <a.stagi@nephila.it>",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/taigaio/taiga-contrib-hipchat/issues"
  },
  "homepage": "https://github.com/taigaio/taiga-contrib-hipchat",
  "dependencies": {
    "coffee-script": "^1.8.0",
    "jade": "^1.9.1"
  }
}
